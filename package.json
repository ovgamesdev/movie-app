{
	"repository": "https://github.com/ovgamesdev/movie-app.git",
	"name": "movie-app",
	"description": "This React Native app supports both TV and mobile.",
	"version": "0.0.1",
	"license": "MIT",
	"private": true,
	"scripts": {
		"android": "react-native run-android --active-arch-only",
		"android:tv": "set ENVFILE=.env.tv && react-native run-android --active-arch-only",
		"build-android": "cd android && ./gradlew assembleRelease",
		"build-android:tv": "cd android && set ENVFILE=.env.tv && ./gradlew assembleRelease",
		"start:reset-cache": "react-native start --experimental-debugger --reset-cache",
		"start:reset-cache-full": "cd android && ./gradlew cleanBuildCache && cd .. && watchman watch-del-all && rm -rf node_modules/ && rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && yarn cache clean && yarn install && yarn start --reset-cache",
		"start": "react-native start --experimental-debugger",
		"start:clean": "yarn clean:packager && react-native start",
		"ios": "react-native run-ios",
		"lint": "eslint .",
		"test": "jest",
		"logcat": "adb logcat '*:W'.",
		"logcat-react": "adb logcat *:S ReactNative:V ReactNativeJS:V"
	},
	"dependencies": {
		"@notifee/react-native": "^7.8.2",
		"@react-native-async-storage/async-storage": "^1.21.0",
		"@react-native-community/netinfo": "^11.2.0",
		"@react-native-google-signin/google-signin": "^10.1.1",
		"@react-native-voice/voice": "^3.2.4",
		"@react-navigation/bottom-tabs": "^6.5.11",
		"@react-navigation/material-top-tabs": "^6.6.5",
		"@react-navigation/native": "^6.1.9",
		"@react-navigation/native-stack": "^6.9.17",
		"@reduxjs/toolkit": "1.9.7",
		"@robinbobin/react-native-google-drive-api-wrapper": "^1.2.3",
		"merge-options": "^3.0.4",
		"react": "^18.2.0",
		"react-native": "npm:react-native-tvos@0.73.0-0",
		"react-native-background-fetch": "^4.2.1",
		"react-native-blob-util": "^0.19.6",
		"react-native-bootsplash": "^5.2.1",
		"react-native-config": "^1.5.1",
		"react-native-error-boundary": "^1.2.4",
		"react-native-gesture-handler": "^2.14.0",
		"react-native-modal": "^13.0.1",
		"react-native-offline": "^6.0.2",
		"react-native-pager-view": "^6.2.3",
		"react-native-reanimated": "^3.6.1",
		"react-native-safe-area-context": "^4.8.0",
