{
	"repository": "https://github.com/ovgamesdev/movie-app.git",
	"name": "movie-app",
	"description": "This React Native app supports both TV and mobile.",
	"version": "0.0.1",
	"license": "MIT",
	"private": true,
	"scripts": {
		"android": "react-native run-android --active-arch-only",
		"android:tv": "set ENVFILE=.env.tv && react-native run-android --active-arch-only",
		"build-android": "cd android && ./gradlew assembleRelease",
		"build-android:tv": "cd android && set ENVFILE=.env.tv && ./gradlew assembleRelease",
		"start:reset-cache": "react-native start --experimental-debugger --reset-cache",
		"start:reset-cache-full": "cd android && ./gradlew cleanBuildCache && cd .. && watchman watch-del-all && rm -rf node_modules/ && rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && yarn cache clean && yarn install && yarn start --reset-cache",
		"start": "react-native start --experimental-debugger",
		"start:clean": "yarn clean:packager && react-native start",
		"ios": "react-native run-ios",
		"lint": "eslint .",
		"test": "jest",
		"logcat": "adb logcat '*:W'.",
		"logcat-react": "adb logcat *:S ReactNative:V ReactNativeJS:V"
	},
	"dependencies": {
		"@notifee/react-native": "^7.8.2",
		"@react-native-async-storage/async-storage": "^1.23.1",
		"@react-native-community/netinfo": "^11.3.1",
		"@react-native-google-signin/google-signin": "^10.1.1",
		"@react-native-voice/voice": "3.2.4",
		"@react-navigation/bottom-tabs": "^6.5.20",
		"@react-navigation/material-top-tabs": "^6.6.13",
		"@react-navigation/native": "^6.1.17",
		"@react-navigation/native-stack": "^6.9.26",
		"@reduxjs/toolkit": "1.9.7",
		"@robinbobin/react-native-google-drive-api-wrapper": "^1.2.4",
		"@shopify/flash-list": "^1.6.4",
		"merge-options": "^3.0.4",
		"react": "^18.2.0",
		"react-native": "npm:react-native-tvos@0.73.6-1",
		"react-native-background-fetch": "^4.2.3",
		"react-native-blob-util": "^0.19.8",
		"react-native-bootsplash": "^5.4.1",
		"react-native-config": "^1.5.1",
		"react-native-error-boundary": "^1.2.4",
		"react-native-gesture-handler": "^2.16.0",
		"react-native-modal": "^13.0.1",
		"react-native-offline": "^6.0.2",
		"react-native-pager-view": "^6.2.3",
		"react-native-reanimated": "^3.8.1",
		"react-native-safe-area-context": "^4.9.0",
		"react-native-screens": "^3.30.1",
		"react-native-svg": "^14.1.0",
		"react-native-tab-view": "^3.5.2",
		"react-native-unistyles": "^2.5.1",
		"react-native-webview": "^13.8.4",
		"react-redux": "8.1.3",
		"rn-update-apk": "^5.0.0",
		"semver": "^7.6.0"
	},
	"devDependencies": {
		"@babel/core": "^7.24.3",
		"@babel/plugin-proposal-decorators": "^7.24.1",
		"@babel/plugin-transform-export-namespace-from": "^7.24.1",
		"@babel/preset-env": "^7.24.3",
		"@babel/runtime": "^7.24.1",
		"@react-native/babel-preset": "^0.74.76",
		"@react-native/metro-config": "^0.73.5",
		"@react-native/typescript-config": "^0.74.76",
		"@types/jest": "^29.5.12",
		"@types/react": "^18.2.73",
		"@types/react-redux": "^7.1.33",
		"@types/react-test-renderer": "^18.0.7",
		"@types/semver": "^7.5.8",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"babel-jest": "^29.7.0",
		"babel-plugin-module-resolver": "^5.0.0",
		"babel-plugin-transform-remove-console": "^6.9.4",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"jest": "^29.7.0",
		"metro-react-native-babel-transformer": "^0.77.0",
		"prettier": "^2.8.8",
		"react-native-svg-transformer": "^1.3.0",
		"react-test-renderer": "18.2.0",
		"reactotron-react-native": "^5.1.4",
		"reactotron-redux": "^3.1.9",
		"typescript": "^5.4.3"
	},
	"jest": {
		"preset": "react-native"
	}
}
